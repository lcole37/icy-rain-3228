<h1> Flights Index Page </h1>
<% @flights.each do |flight| %>
  <li> <%= flight.number %>: <%= flight.airline.name %> </li>
  <% flight.passengers.each do |passenger| %>
    <p>
       <%= passenger.name %>
       <%= link_to "delete", "/flights/#{passenger.id}", method: :delete %>
    </p>
  <% end %>
<% end %>
